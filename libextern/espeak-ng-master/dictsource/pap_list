// common function words

un	$u+	// a
algun	$u+	// some
to

i	$u+ $pause	// and
y	$u  $pause	// and ??
y	igR'Ek	$atend

o	$u+ $pause	// or
òf	$u+ $pause	// or
ma	$pause	// but
ku	$u+ $brk	// which, that
(ken su) $u $brk


mi	$u+	// I,me
bo	$u+	// you
e	$u+	// the. it, he, she
el	$u+	// it, he, she (before 'a')
nos	$u+	// we
boso	$u+	// you
nan	$u+	// they

ami	$u+	// I
abo	$u+	// you
su	$u+	// his, her, its


ta	$u+	// to be
tabata	$u+	// was
lo	$u+	// (future)
a	$u+	// (past)
// tin	$u+	// have
por	$u+	// can

di	$u+	// of
den	$u+	// in
na	$u+	// at

