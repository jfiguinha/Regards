// characters
b	be
c	se
d	de
f	Ef
g	ge
h	ha
j	je
k	ka
l	El
m	Em
n	En
p	pe
q	ky
r	ER
s	Es
t	te
v	ve
w	we
x	Eks
z	zEt
ñ	en^e

_a	a
_e	e
_i	i
_o	o
_y	igR'Ek	$atend

// these letters are in pap_rules, but the letter names include the accent name
á	$accent
é	$accent
í	$accent
ó	$accent
ú	$accent
// è	$accent
// ò	$accent
// ù	$accrnt

// accent names
_acu	skERpi
_grv	gRave


// numbers
_0	z'eRo
_1	'unu
