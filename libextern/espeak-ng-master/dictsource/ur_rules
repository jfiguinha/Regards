// Common symbols

.group
	$	do:l@R
	,	sakta	// 	Sakta سکتہ
	!	f'iJa:ja	//Fijaya 	فجائیہ
	;	vakfa	//Waqfa 	وقفہ
	:	r'a:b'tah//Rabtar 	رابطہ
	?	saw'a:l'i:ja:n'is'a:nV	// Sawaliya Nishan	سوالیہ نشان
	%	f'i:sVd	//Fisad 	فیصد
	۔	xatma	//Khatma 	ختمہ
	؟	sava:li:;a:||nISa:n	//Sawalya Nishan 	سوالیہ نشان
	؛	vaqfa	//waqfa	وقفہ
	،	sakta	// 	Sakta سکتہ
	؏	mIs.Ra:	// 	misra مصرع
	؎	naz.m||nISa:n	// 	nazm nishan نظم نشان



.group 0xe0a4  // devanagari
	0xe0a4  _^_NE  // switch to Nepali voice
.group 0xe0a5  // devanagari
	0xe0a5  _^_NE

