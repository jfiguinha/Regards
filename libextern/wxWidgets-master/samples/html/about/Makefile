data_files: 
	@mkdir -p ./data
	@for f in about.htm logo.png; do \
	if test ! -f ./data/$$f -a ! -d ./data/$$f ; \
	then x=yep ; \
	else x=`find $(srcdir)/data/$$f -newer ./data/$$f -print` ; \
	fi; \
	case "$$x" in ?*) \
	cp -pRf $(srcdir)/data/$$f ./data ;; \
	esac; \
	done

about_sample_rc.o: $(srcdir)/../../../samples/sample.rc
	$(WINDRES) -i$< -o$@    --define __WX$(TOOLKIT)__ $(__WXUNIV_DEFINE_p_1) $(__DEBUG_DEFINE_p_1)  $(__EXCEPTIONS_DEFINE_p_1) $(__RTTI_DEFINE_p_1) $(__THREAD_DEFINE_p_1)  --include-dir $(srcdir) $(__DLLFLAG_p_1) --include-dir $(srcdir)/../../../samples $(__RCDEFDIR_p) --include-dir $(top_srcdir)/include

about_about.o: $(srcdir)/about.cpp
	$(CXXC) -c -o $@ $(ABOUT_CXXFLAGS) $(srcdir)/about.cpp


