echo /home/figuinha/developpement/git/Regards/libextern/MediaInfo_DLL_GNU_FromSource/ZenLib/Project/GNU/Library/.libs/libzen.a -lpthread -lstdc++ 
fi
if test "$1" = "Unicode"; then
echo yes
fi
if test "$1" = "WstringMissing"; then
